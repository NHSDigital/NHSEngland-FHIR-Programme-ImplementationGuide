<Bundle xmlns="http://hl7.org/fhir">
    <id value="RemoveIndividualRARecord-Bundle-Example" />
    <type value="transaction" />

    <!-- Delete a specific RA Adjustment Flag -->
    <entry>
        <request>
            <method value="DELETE" />
            <url value="Flag/RA-Flag-Example" />
        </request>
    </entry>

    <!-- Delete a specific RA Condition -->
    <entry>
        <request>
            <method value="DELETE" />
            <url value="Condition/RA-Condition-Example" />
        </request>
    </entry>

    <!-- Optional: Delete associated RA Consent -->
    <entry>
        <request>
            <method value="DELETE" />
            <url value="Consent/RA-Consent-Example" />
        </request>
    </entry>
</Bundle>
